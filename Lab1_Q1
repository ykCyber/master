package day14_Methods_Part2;

public class Lab1_Q1 {

	public static void main(String[] args) {
//		Write a method that accepts a string and a non-negative number, and prints a larger
//		string that is number copies of the original String
//		stringTimes("Hi", 2); = > HiHi
//		stringTimes("Hi", 3); = > HiHiHi
//		stringTimes("Hi", 1); = > Hi
		
		stringTimes("Hi", 2);
	
		
	}
	public static void stringTimes(String message, int num) {
		
		for (int i=0; i<num; i++) {
		System.out.print(message);
		}
		System.out.println();
	}

}
